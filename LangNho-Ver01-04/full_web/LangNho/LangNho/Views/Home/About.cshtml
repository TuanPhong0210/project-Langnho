<!--=================================
  Header Inner -->
<section class="header-inner header-inner-menu bg-overlay-secondary"
         style="background-image: url('../../Content/images/listing/06.jpg');">
    <div class="container">
        <div class="row d-flex justify-content-center">
            <div class="col-md-12 position-relative">
                <div class="header-inner-title">
                    <div class="section-title">

                        <h2 class="title text-white"><strong>About Us</strong></h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="header-inner-shape" style="background-image: url('../../Content/images/bg/02.png');"></div>
